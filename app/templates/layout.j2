			<link href="{{url_for('static', filename='vendor/aos/aos.css')}}" rel="stylesheet">
			<!-- Template Main CSS File -->
			<link rel="stylesheet" href="{{url_for('static', filename='css/style.css')}}">
			<link rel="stylesheet" href="{{url_for('static', filename='css/layout.css')}}">
			<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Julius+Sans+One">
			<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
		{% endblock %}
	</head>
	<body>
		<!-- ======= Header ======= -->
		<header id="header" class="fixed-top ">
			<div class="container-fluid">
				<div class="row justify-content-center">
					<div class="col-xl-9 d-flex align-items-center">
						<a href="index.html" class="logo mr-auto"><img src="{{url_for('static', filename='img/logo.png')}}" alt="" class="img-fluid" style="height:50px; width:75px; padding-bottom: 5px !important;"></a>
						<h1 class="logo mr-auto"><a href="index.html" style="font-family: Julius Sans One">Pil&#9881tech</a></h1>
						
						<nav class="nav-menu d-none d-lg-block">
							<ul>
								<li class="active">
									<a href="index.html">Home</a>
								</li>
								<li>
									<a class="nav-link" href="{{url_for('services.services_landing_page')}}">Services</a>
								</li>
								<li>
									<a class="nav-link" href="{{url_for('demo.demo_landing_page')}}">Demo</a>
								</li>
								<li>
									<a class="nav-link" href="{{url_for('pricing.pricing_landing_page')}}">Pricing</a>
								</li>
								<li>
									<a class="nav-link" href="{{url_for('api.api_landing_page')}}">API</a>
								</li>
								<li>
									<a class="nav-link" href="{{url_for('about_us.about_us_landing_page')}}">About Us</a>
								</li>
								<li>
									<a class="nav-link" href="{{url_for('login.login_landing_page')}}">Login</a>
								</li>
							</ul>
							</nav><!-- .nav-menu -->
							<a href="#about" class="get-started-btn scrollto">Get Started</a>
						</div>
					</div>
				</div>
			</header>
			{% block content %}
			{% endblock %}
			{% block footer %}
				<!-- ======= Footer ======= -->
				<footer id="footer">
					<div class="footer-top">
						<div class="container">
							<div class="row">
								<div class="col-lg-3 col-md-6 footer-contact">
									<h3>PILOTECH</h3>
									<p>
										San Francisco Bay Area <br><br>
										<strong>Email:</strong> mishalubich007@berkeley.edu<br>
										etyryshkin@ucdavis.edu <br><br>
									</p>
								</div>
								<div class="col-lg-2 col-md-6 footer-links">
									<h4>Useful Links</h4>
									<ul>
										<li><i class="bx bx-chevron-right"></i> <a href="#">Home</a></li>
										<li><i class="bx bx-chevron-right"></i> <a href="#">Services</a></li>
										<li><i class="bx bx-chevron-right"></i> <a href="#">Demo</a></li>
										<li><i class="bx bx-chevron-right"></i> <a href="#">Pricing</a></li>
										<li><i class="bx bx-chevron-right"></i> <a href="#">About Us</a></li>
										<li><i class="bx bx-chevron-right"></i> <a href="#">Login</a></li>
									</ul>
								</div>
								<div class="col-lg-3 col-md-6 footer-links">
									<h4>Our Services</h4>
									<ul>
										<li><i class="bx bx-chevron-right"></i> <a href="#">Machine Learning</a></li>
